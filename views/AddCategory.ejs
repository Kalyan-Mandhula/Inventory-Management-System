
<% layout('./BootStrap/Layout') %>


<div class="d-flex justify-content-center my-5">
<form class="col-5 needs-validation " action="/inventory/AddCategory" method="post"  novalidate >   
    <div class="mb-3 mt-3">
      <h3>New Category</h3>
    </div>
    <div class="mb-3 mt-3 ">
      <label for="Title" class="form-label">Name</label>
      <input type="text" class="form-control " id="Title" name="category" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
</div>
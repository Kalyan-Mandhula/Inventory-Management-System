<% layout('./BootStrap/Layout') %>
<%- include("./Bootstrap/successFlash") %>
<div class="d-flex justify-content-center my-1">
<form action="/inventory/login" method="POST" novalidate class="needs-validation col-md-6 col-lg-4 col-sm-8 mt-3 shadow p-3 mb-5 bg-white rounded">

  <div class="mb-3 ">
    <h5 class="text-center" >Please login to your account</h5>
  </div>
  <div class="mb-3 mt-1 ">
    <img class="card-img-top " src="data:image/jpeg;base64,">
</div>

    <div class="form-group mt-3 ">
      <input name="username" type="text" class="form-control mt-2" id="username" placeholder="username" required>

      <div class="valid-feedback">
        Looks good!
      </div>

    </div>

    <div class="form-group mt-3 ">
      <input name="password" type="password" class="form-control mt-2" id="password" placeholder="password" required>

      <div class="valid-feedback">
        Looks good!
      </div>

    </div>
    <div class="form-group my-3">
      <button type="submit" class="btn btn-primary">Login</button>
    </div>

    <div class="form-group mt-3 mb-5">
        <h5>Don't have an account?<a class="text-danger text-decoration-none pe-auto" href="/inventory/register"> Register</a> </h5>
    </div>
   
   
  </form>
</div>


